<h1>Risposta</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Descrizione</th>
            <td>{{ entity.descrizione }}</td>
        </tr>
        <tr>
            <th>Vero</th>
            <td>{{ entity.vero }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('risposta') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('risposta_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('risposta_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
